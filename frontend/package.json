{
  "name": "frontend",
  "proxy": "http://localhost:5000",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "deploy": "react-scripts build && react-snapshot && firebase deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "all": "^0.0.0",
    "axios": "^0.21.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-github-btn": "^1.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "react-snapshot": "^1.3.0"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "/aboutus",
      "/process",
      "/services",
      "/expect",
      "/insurance-fees",
      "/locations",
      "/appointments",
      "/resources",
      "/privacy-policy",
      "/terms-conditions"
    ],
    "exclude": [
      "/login",
      "/signup",
      "/profile",
      "/listings/**",
      "/new_listing"
    ],
    "snapshotDelay": 300
  }
}
