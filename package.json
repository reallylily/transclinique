  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "create-react-app": "^3.4.1",
    "express": "^4.16.4",
    "frontend": "^2.0.0-alpha.4",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.11.15",
    "node-sass": "^4.14.1",
    "npm": "^6.14.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
